<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:stv="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="cn.com.stableloan.ui.fragment.information.User_MeansFragment">

<LinearLayout
    android:orientation="vertical"
    android:background="@color/base_white"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/layout_camera"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        stv:sLeftTextSize="@dimen/information_textSize"
        stv:sLeftTextString="扫描身份证快速录入"
        stv:sRightIconRes="@mipmap/ic_arrow"
        stv:sRightTextColor="#ff0000"
        stv:sRightTextSize="12dp"
        stv:sRightTvDrawableRight="@mipmap/iv_scan"
        stv:sBottomDividerLineMarginLeft="10dp"
        stv:sBottomDividerLineMarginRight="10dp"

        stv:sDividerLineHeight="0dp"
        />

    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/et_name"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        stv:sLeftTextSize="@dimen/information_textSize"
        stv:sLeftTextString="真实姓名"
        stv:sMaxEms="13"

        stv:sRightIconRes="@mipmap/ic_arrow"
        stv:sRightTextColor="@color/text_setting"
        stv:sLeftTextColor="@color/select_text_color"
        stv:sRightTextSize="@dimen/information_textSize"
        stv:sBottomDividerLineMarginLeft="10dp"
        stv:sBottomDividerLineMarginRight="10dp"

        stv:sRightTextString="姓名"
        />
    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/user_phone"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        stv:sLeftTextSize="@dimen/information_textSize"
        stv:sLeftTextString="绑定手机号"
        stv:sRightTextColor="@color/text_setting"
        stv:sLeftTextColor="@color/select_text_color"
        stv:sRightTextSize="@dimen/information_textSize"
        stv:sBottomDividerLineMarginLeft="10dp"
        stv:sBottomDividerLineMarginRight="10dp"
        />
    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/et_IDCard"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        stv:sLeftTextSize="@dimen/information_textSize"
        stv:sLeftTextString="身份证"
        stv:sRightIconRes="@mipmap/ic_arrow"
        stv:sRightTextColor="@color/text_setting"
        stv:sLeftTextColor="@color/select_text_color"
        stv:sRightTextSize="@dimen/information_textSize"
        stv:sBottomDividerLineMarginLeft="10dp"
        stv:sBottomDividerLineMarginRight="10dp"

        />
    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/et_Sex"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        stv:sLeftTextSize="@dimen/information_textSize"
        stv:sLeftTextString="性别"
        stv:sRightTextColor="@color/text_setting"
        stv:sLeftTextColor="@color/select_text_color"
        stv:sRightTextSize="@dimen/information_textSize"
        stv:sBottomDividerLineMarginLeft="10dp"
        stv:sBottomDividerLineMarginRight="10dp"

        stv:sRightTextString="男/女"
        />
    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/et_Age1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        stv:sLeftTextSize="@dimen/information_textSize"
        stv:sLeftTextString="年龄"
        stv:sRightIconRes="@mipmap/ic_arrow"
        stv:sRightTextColor="@color/text_setting"
        stv:sLeftTextColor="@color/select_text_color"
        stv:sRightTextSize="@dimen/information_textSize"
        stv:sBottomDividerLineMarginLeft="10dp"
        stv:sBottomDividerLineMarginRight="10dp"

        />
    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/et_Address"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        stv:sLeftTextSize="@dimen/information_textSize"
        stv:sLeftTextString="身份证地址"
        stv:sRightIconRes="@mipmap/ic_arrow"
        stv:sRightTextColor="@color/text_setting"
        stv:sLeftTextColor="@color/select_text_color"
        stv:sRightTextSize="@dimen/information_textSize"
        stv:sMaxEms="13"
        stv:sBottomDividerLineMarginLeft="10dp"
        stv:sBottomDividerLineMarginRight="10dp"

        />
    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/et_Marriage"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        stv:sLeftTextSize="@dimen/information_textSize"
        stv:sLeftTextString="婚姻状况"
        stv:sRightIconRes="@mipmap/ic_arrow"
        stv:sRightTextColor="@color/text_setting"
        stv:sLeftTextColor="@color/select_text_color"
        stv:sRightTextSize="@dimen/information_textSize"
        stv:sBottomDividerLineMarginLeft="10dp"
        stv:sBottomDividerLineMarginRight="10dp"

        />
    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/et_City"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        stv:sLeftTextSize="@dimen/information_textSize"
        stv:sLeftTextString="所在城市"
        stv:sRightTextColor="@color/text_setting"
        stv:sLeftTextColor="@color/select_text_color"
        stv:sRightTextSize="@dimen/information_textSize"
        stv:sBottomDividerLineMarginLeft="10dp"
        stv:sBottomDividerLineMarginRight="10dp"

        stv:sRightTextString="点击自动获取"
        />
    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/et_Contact_name"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        stv:sLeftTextSize="@dimen/information_textSize"
        stv:sLeftTextString="紧急联系人1"
        stv:sRightTextColor="@color/text_setting"
        stv:sLeftTextColor="@color/select_text_color"
        stv:sRightTextSize="@dimen/information_textSize"
        stv:sBottomDividerLineMarginLeft="10dp"
        stv:sBottomDividerLineMarginRight="10dp"

        stv:sRightTextString="点击选择联系人"
        />
    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/et_Contact1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        stv:sLeftTextSize="@dimen/information_textSize"
        stv:sLeftTextString="联系人电话"
        stv:sRightIconRes="@mipmap/ic_arrow"
        stv:sRightTextColor="@color/text_setting"
        stv:sLeftTextColor="@color/select_text_color"
        stv:sRightTextSize="@dimen/information_textSize"
        stv:sBottomDividerLineMarginLeft="10dp"
        stv:sBottomDividerLineMarginRight="10dp"

        stv:sRightTextString="18500634223"
        />

    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/et_Between1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        stv:sLeftTextSize="@dimen/information_textSize"
        stv:sLeftTextString="与您的关系"
        stv:sRightIconRes="@mipmap/ic_arrow"
        stv:sRightTextColor="@color/text_setting"
        stv:sLeftTextColor="@color/select_text_color"
        stv:sRightTextSize="@dimen/information_textSize"
        stv:sBottomDividerLineMarginLeft="10dp"
        stv:sBottomDividerLineMarginRight="10dp"

        />
    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/et_Contact_name2"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        stv:sLeftTextSize="@dimen/information_textSize"
        stv:sLeftTextString="紧急联系人2"
        stv:sRightTextColor="@color/text_setting"
        stv:sLeftTextColor="@color/select_text_color"
        stv:sRightTextSize="@dimen/information_textSize"
        stv:sBottomDividerLineMarginLeft="10dp"
        stv:sBottomDividerLineMarginRight="10dp"

        stv:sRightTextString="点击选择联系人"
        />
    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/et_Contact"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        stv:sLeftTextSize="@dimen/information_textSize"
        stv:sLeftTextString="联系人电话"
        stv:sRightIconRes="@mipmap/ic_arrow"
        stv:sRightTextColor="@color/text_setting"
        stv:sLeftTextColor="@color/select_text_color"
        stv:sRightTextSize="@dimen/information_textSize"
        stv:sBottomDividerLineMarginLeft="10dp"
        stv:sBottomDividerLineMarginRight="10dp"
        />
    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/et_Between2"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        stv:sLeftTextSize="@dimen/information_textSize"
        stv:sLeftTextString="与您的关系"
        stv:sRightIconRes="@mipmap/ic_arrow"
        stv:sRightTextColor="@color/text_setting"
        stv:sLeftTextColor="@color/select_text_color"
        stv:sRightTextSize="@dimen/information_textSize"
        stv:sBottomDividerLineMarginLeft="10dp"
        stv:sBottomDividerLineMarginRight="10dp"

        />
    <cn.com.stableloan.view.RoundButton
        android:id="@+id/save"
        android:layout_marginBottom="50dp"
        android:layout_marginTop="40dp"
        android:layout_gravity="center"
        android:layout_width="180dp"
        android:layout_height="40dp"
        android:text="保 存"
        app:btnCornerRadius="5dp"
        app:btnPressedRatio="0.9"
        app:btnSolidColor="@color/colorPrimary"
        android:gravity="center"
        android:textSize="18sp"
        android:textColor="@color/white"
        />
</LinearLayout>
</ScrollView>
