<?xml version="1.0" encoding="utf-8"?>
<cn.com.stableloan.view.statuslayout.StateLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/stateLayout"
    tools:context="cn.com.stableloan.ui.activity.CollectionActivity">
    <include layout="@layout/toolbar_layout"/>
    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/SwipeLayout"
        android:layout_marginTop="50dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <android.support.v7.widget.RecyclerView
            android:id="@+id/recyclerView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        </android.support.v7.widget.RecyclerView>
    </android.support.v4.widget.SwipeRefreshLayout>
</cn.com.stableloan.view.statuslayout.StateLayout>
