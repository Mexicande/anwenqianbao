<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:stv="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@color/base_white"
    tools:context="cn.com.stableloan.ui.activity.ImageActivity">
    <include layout="@layout/goback_toolbar"/>
    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/identity"
        android:layout_marginTop="@dimen/_10dp"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        stv:sLeftBottomTextColor="@color/gay"
        stv:sLeftBottomTextSize="15sp"
        stv:sLeftBottomTextString="申请人必填信息"
        stv:sLeftIconRes="@mipmap/iv_identity_image"
        stv:sLeftTextColor="@android:color/black"
        stv:sLeftTextSize="16sp"
        stv:sLeftTextString="身份证"
        stv:sLeftTvDrawableWidth="30dp"
        stv:sRightTextString="未完成"
        stv:sLeftViewMarginLeft="15dp"
        stv:sRightTextColor="@color/white"
        stv:sRightTextSize="17sp"
        stv:sRightTvDrawableWidth="20dp"
        stv:sRightTvDrawableHeight="20dp"
        stv:sTextViewDrawablePadding="10dp"
        stv:sRightViewMarginRight="15dp"
        stv:sLeftViewGravity="left_center" />
    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/bank"
        android:layout_marginTop="@dimen/_10dp"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        stv:sLeftBottomTextColor="@color/gay"
        stv:sLeftBottomTextSize="15sp"
        stv:sLeftBottomTextString="有可能用到的第三方信息"
        stv:sLeftIconRes="@mipmap/iv_cashbank"
        stv:sLeftTextColor="@android:color/black"
        stv:sLeftTextSize="16sp"
        stv:sLeftTextString="银行卡"
        stv:sLeftTvDrawableWidth="30dp"
        stv:sRightTextString="未完成"
        stv:sLeftViewMarginLeft="15dp"
        stv:sRightTextColor="@color/white"
        stv:sRightTextSize="17sp"
        stv:sRightTvDrawableWidth="20dp"
        stv:sRightTvDrawableHeight="20dp"
        stv:sTextViewDrawablePadding="10dp"
        stv:sRightViewMarginRight="15dp"
        stv:sLeftViewGravity="left_center" />
    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/CreditBank"
        android:layout_marginTop="@dimen/_10dp"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        stv:sLeftBottomTextColor="@color/gay"
        stv:sLeftBottomTextSize="15sp"
        stv:sLeftBottomTextString="有可能需要的图片材料"
        stv:sLeftIconRes="@mipmap/iv_creditbank"
        stv:sLeftTextColor="@android:color/black"
        stv:sLeftTextSize="16sp"
        stv:sLeftTextString="信用卡"
        stv:sLeftTvDrawableWidth="30dp"
        stv:sRightTextString="未完成"
        stv:sLeftViewMarginLeft="15dp"
        stv:sRightTextColor="@color/white"
        stv:sRightTextSize="17sp"
        stv:sRightTvDrawableWidth="20dp"
        stv:sRightTvDrawableHeight="20dp"
        stv:sTextViewDrawablePadding="10dp"
        stv:sRightViewMarginRight="15dp"
        stv:sLeftViewGravity="left_center" />
    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/camp"
        android:layout_marginTop="@dimen/_10dp"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        stv:sLeftBottomTextColor="@color/gay"
        stv:sLeftBottomTextSize="15sp"
        stv:sLeftBottomTextString="有可能需要的图片材料"
        stv:sLeftIconRes="@mipmap/iv_campbusiness"
        stv:sLeftTextColor="@android:color/black"
        stv:sLeftTextSize="16sp"
        stv:sLeftTextString="营业执照"
        stv:sLeftTvDrawableWidth="30dp"
        stv:sRightTextString="未完成"
        stv:sLeftViewMarginLeft="15dp"
        stv:sRightTextColor="@color/white"
        stv:sRightTextSize="17sp"
        stv:sRightTvDrawableWidth="20dp"
        stv:sRightTvDrawableHeight="20dp"
        stv:sTextViewDrawablePadding="10dp"
        stv:sRightViewMarginRight="15dp"
        stv:sLeftViewGravity="left_center" />
    <cn.com.stableloan.view.supertextview.SuperTextView
        android:id="@+id/userCard"
        android:layout_marginTop="@dimen/_10dp"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        stv:sLeftBottomTextColor="@color/gay"
        stv:sLeftBottomTextSize="15sp"
        stv:sLeftBottomTextString="有可能需要的图片材料"
        stv:sLeftIconRes="@mipmap/iv_usercard"
        stv:sLeftTextColor="@android:color/black"
        stv:sLeftTextSize="16sp"
        stv:sLeftTextString="名片或工牌"
        stv:sLeftTvDrawableWidth="30dp"
        stv:sRightTextString="未完成"
        stv:sLeftViewMarginLeft="15dp"
        stv:sLeftIconMarginLeft="15dp"
        stv:sRightTextColor="@color/white"
        stv:sRightTextSize="17sp"
        stv:sRightTvDrawableWidth="20dp"
        stv:sRightTvDrawableHeight="20dp"
        stv:sTextViewDrawablePadding="10dp"
        stv:sRightViewMarginRight="15dp"
        stv:sLeftViewGravity="left_center" />

</LinearLayout>
