<?xml version="1.0" encoding="utf-8"?>
<resources>




    <!--图片上传进度-->
    <declare-styleable name="ImageProgress">
        <attr name="text_size" format="dimension" />
    </declare-styleable>

    <!--图片上传进度-->
    <declare-styleable name="ShapeButton">
        <attr name="buttonColor" format="reference" />
        <attr name="radius" format="dimension" />
        <attr name="stroke" format="dimension" />
        <attr name="shapeStyle" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ButtonBgUi">
        <attr name="strokeColor" format="color"/>
        <attr name="selectStrokeColor" format="color"/>
        <attr name="pressedColor" format="color"/>
        <attr name="defaultColor" format="color"/>
        <attr name="isRipple" format="boolean"/>
        <attr name="parameter" format="float"/>
        <attr name="strokeWidth" format="dimension"/>
        <attr name="raoundRadius" format="dimension"/>
        <attr name="topLeftRadius" format="dimension"/>
        <attr name="topRightRadius" format="dimension"/>
        <attr name="bottomLeftRadius" format="dimension"/>
        <attr name="bottomRightRadius" format="dimension"/>

    </declare-styleable>

    <declare-styleable name="StateLayout">
        <attr name="errorDrawable" format="reference" />
        <attr name="emptyDrawable" format="reference" />
        <attr name="progressView" format="reference" />
    </declare-styleable>


    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="AutofitTextView">
        <!-- Minimum size of the text. -->
        <attr name="minTextSize" format="dimension" />
        <!-- Amount of precision used to calculate the correct text size to fit within its
        bounds. Lower precision is more precise and takes more time. -->
        <attr name="precision" format="float" />
        <!-- Defines whether to automatically resize text to fit to the view's bounds. -->
        <attr name="sizeToFit" format="boolean" />
    </declare-styleable>
</resources>